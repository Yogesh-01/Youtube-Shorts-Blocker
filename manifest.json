{
    "manifest_version": 2,
    "name": "Youtube Shorts Blocker",
    "version": "1.0",
    "content_scripts": [
         {
             "matches":["https://www.youtube.com/*"],
             "js": ["content.js"]
         }
     ],
     "background":{
         "scripts":["background.js"]
     },
     "browser_action":{
         "default_icon": "icon.png",
         "default_popup": "popup.html",
         "default_title": "popup hehe"
     }
}